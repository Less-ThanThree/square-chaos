; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Square-chaos"
run/main_scene="res://Scenes/Game/Game.tscn"
config/features=PackedStringArray("4.1", "Mobile")
config/icon="res://icon.svg"

[autoload]

DeviceInfo="*res://GlobalScripts/DeviceInfo.gd"
GeneratorLevel="*res://GlobalScripts/GeneratorLevel.gd"
PlayerStatus="*res://GlobalScripts/PlayerStatus.gd"
ProbabilityBank="*res://GlobalScripts/ProbabilityBank.gd"

[display]

window/size/viewport_width=720
window/size/viewport_height=1440
window/stretch/mode="canvas_items"
window/handheld/orientation=5

[rendering]

renderer/rendering_method="mobile"
textures/vram_compression/import_etc2_astc=true
